<h1 class="text-center mb-4">Data Users</h1>

<table class="table table-info border border-dark">
    <thead>
        <tr>
            <th scope="col">No</th>
            <th scope="col">Nama</th>
            <th scope="col">Email</th>
            <th scope="col">Aksi</th>
        </tr>
    </thead>
    <tbody>
        <% let no = 0; %>
        <% datas.forEach(data => { no++ %>
         <tr>
            <th scope="row"><%= no %></th>
            <td><%= data.nama %></td>
            <td><%= data.email %></td>
            <td><a href="/users/detail/<%= data.id %>" class="btn btn-primary badge"><i class="bi bi-info-circle-fill"></i> detail</a></td>
        </tr>
        <% }) %>
        

    </tbody>
</table>